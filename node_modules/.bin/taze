#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules/taze/bin/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules/taze/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules/taze/bin/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules/taze/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/taze@0.11.2/node_modules:/Users/hongje/Documents/dev/portfolio-macos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../taze/bin/taze.mjs" "$@"
else
  exec node  "$basedir/../taze/bin/taze.mjs" "$@"
fi
